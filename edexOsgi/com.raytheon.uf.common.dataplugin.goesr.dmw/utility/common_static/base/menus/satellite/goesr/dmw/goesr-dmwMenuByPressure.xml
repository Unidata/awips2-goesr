<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
   <!-- 
        This is an absolute override file, indicating that a higher priority 
        version of the file will completely replace a lower priority version
        of the file. 
    -->
    <!-- TOWRdocs Header
         DMW menu by pressure.
    -->
    <!-- TOWRdocs Description
Sets the menus for by-pressure DMWs.  Modifies previous baseline file to add additional legend information after the pressure range.
    -->
    <!-- TOWRdocs Status
         An updated menu file for by-pressure DMWs. Contributed by Bill Line of WFO Pueblo.
   -->
    <!-- TOWRdocs POC
         Lee Byerle 1/15/2018
    -->

<menuTemplate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <!-- 200 mb -->
    <contribute xsi:type="bundleItem" file="bundles/satellite/goesr/goesr-dmwBundleByPressure-200.xml" 
           menuText="000-250mb" id="goesr-DMW-Pressure-200mb">
        <substitute key="legend" value="000-250mb ${legend1}"/>
        <substitute key="pressureConstraint" value="BETWEEN" />
        <substitute key="pressure" value="0--250" />
    </contribute>

    <!-- 300 mb -->
