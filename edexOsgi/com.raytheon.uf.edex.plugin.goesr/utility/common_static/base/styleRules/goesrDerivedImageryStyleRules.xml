            <values>-1 -0.5 -0.25 -0.10 0.00 0.25 0.50 0.75 1.00</values>
            </colorbarLabeling>
        </imageStyle>
    </styleRule>
   <styleRule>
        <paramLevelMatches>
            <parameter>satNDVI</parameter>
        </paramLevelMatches>
        <imageStyle>
            <range scale="LINEAR">
                <minValue>-1.0</minValue>
