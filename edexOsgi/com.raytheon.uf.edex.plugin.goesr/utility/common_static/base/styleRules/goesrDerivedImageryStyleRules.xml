      <displayUnits>%</displayUnits> 
      <range scale="LINEAR">
        <minValue>50</minValue>
        <maxValue>-25</maxValue>
      </range>
      <defaultColormap>GOES-R/IR/enhanced-rainbow-11</defaultColormap>
      <colorbarLabeling>
        <values>40 30 20 10 0 -10 -20</values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>
    <styleRule>
        <paramLevelMatches>
            <parameter>satSnow</parameter>
