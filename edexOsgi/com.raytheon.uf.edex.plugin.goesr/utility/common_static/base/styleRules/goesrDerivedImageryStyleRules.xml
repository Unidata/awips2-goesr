   <!--
        This is an incremental override file, indicating that the files
        at different localization levels will be combined.
    -->
<styleRuleset>
  <!-- GOES-R Derived Satellite Rules -->
<styleRule>
    <paramLevelMatches>
      <parameter>satMoisture</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>K</displayUnits>
      <range scale="LINEAR">
        <minValue>-15</minValue>
        <maxValue>15</maxValue>
      </range>
      <defaultColormap>GOES-R/IR/dust_and_moisture_split_window</defaultColormap>
      <colorbarLabeling>
        <values>-12 -8 -4 0 4 8 12 </values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>
    <styleRule>
        <paramLevelMatches>
            <parameter>satCloudPhase</parameter>
        </paramLevelMatches>
        <imageStyle>
            <displayUnits label="C">K</displayUnits>
            <range scale="LINEAR">
                <minValue>-8.0</minValue>
                <maxValue>24.0</maxValue>
            </range>
            <defaultColormap>Grid/Gridded Data</defaultColormap>
            <colorbarLabeling>
                <values>-5 0 5 10 15 20</values>
            </colorbarLabeling>
        </imageStyle>
    </styleRule>
<styleRule>
    <paramLevelMatches>
      <parameter>satFog</parameter>
      <parameter>satFog2</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>K</displayUnits> 
      <range scale="LINEAR">
        <minValue>-15</minValue>
        <maxValue>15</maxValue>
      </range>
      <defaultColormap>GOES-R/IR/fogdiff_blue</defaultColormap>
      <colorbarLabeling>
        <values>-12 -8 -4 0 4 8 12 </values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>
<styleRule>
    <paramLevelMatches>
      <parameter>satFog2</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>K</displayUnits> 
      <range scale="LINEAR">
        <minValue>-15</minValue>
        <maxValue>15</maxValue>
      </range>
      <defaultColormap>GOES-R/IR/fogdiff_blue</defaultColormap>
      <colorbarLabeling>
        <values>-12 -8 -4 0 4 8 12 </values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>
<styleRule>
    <paramLevelMatches>
      <parameter>satDayFog</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>K</displayUnits> 
      <range scale="LINEAR">
        <minValue>-5</minValue>
        <maxValue>60</maxValue>
      </range>
      <defaultColormap>Grid/Gridded Data</defaultColormap>
      <colorbarLabeling>
        <values>-5 0 5 10 20 30 40 50 </values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>
<styleRule>
