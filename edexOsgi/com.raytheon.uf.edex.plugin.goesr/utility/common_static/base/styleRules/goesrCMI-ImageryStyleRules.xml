<styleRuleset>
  <!-- GOES-R CMI Satellite Rules -->
  <styleRule>
    <paramLevelMatches>
      <parameter>CH-11-8.50um</parameter>
      <parameter>CH-12-9.61um</parameter>
      <parameter>CH-13-10.35um</parameter>
      <parameter>CH-14-11.20um</parameter>
      <parameter>CH-15-12.30um</parameter>
      <parameter>CH-16-13.30um</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>C</displayUnits>
      <range scale="LINEAR">
        <minValue>55</minValue>
        <maxValue>-109</maxValue>
      </range>
      <defaultColormap>GOES-R/IR/IR_Color_Clouds_Winter</defaultColormap>
      <colorbarLabeling>
        <values>40 20 0 -20 -40 -60 -80</values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>
  <styleRule>
    <paramLevelMatches>
      <parameter>CH-07-3.90um</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>C</displayUnits>
      <range scale="LINEAR">
        <minValue>127</minValue>
        <maxValue>-109</maxValue>
      </range>
      <defaultColormap>GOES-R/IR/enhanced-rainbow_warmer_yellow</defaultColormap>
      <colorbarLabeling>
        <values>100 70 40 10 -20 -50 -80</values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>

  <styleRule>
    <paramLevelMatches>
      <parameter>CH-08-6.19um</parameter>
      <parameter>CH-09-6.95um</parameter>
      <parameter>CH-10-7.34um</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>C</displayUnits>
      <range scale="LINEAR">
        <minValue>55</minValue>
        <maxValue>-109</maxValue>
      </range>
      <defaultColormap>GOES-R/IR/ramsdis_WV_12bit</defaultColormap>
      <colorbarLabeling>
        <values>40 20 0 -20 -40 -60 -80</values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>

  <styleRule>
    <paramLevelMatches>
      <parameter>CH-01-0.47um</parameter>
      <parameter>CH-02-0.64um</parameter>
      <parameter>CH-03-0.87um</parameter>
      <parameter>CH-05-1.61um</parameter>
      <parameter>CH-06-2.25um</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>%</displayUnits>
      <range scale="LINEAR">
        <minValue>0</minValue>
        <maxValue>130</maxValue>
      </range>
      <defaultColormap>GOES-R/VIS/VIS_gray_sq-root-12</defaultColormap>
      <colorbarLabeling>
        <values>20 40 60 80 100 120</values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>

  <styleRule>
    <paramLevelMatches>
      <parameter>CH-04-1.38um</parameter> 
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>%</displayUnits>
      <range scale="LINEAR">
        <minValue>0</minValue>
        <maxValue>50</maxValue>
      </range>
      <defaultColormap>GOES-R/VIS/VIS_gray_sq-root-12</defaultColormap>
      <colorbarLabeling>
        <values>10 20 30 40 50</values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>

<styleRule>
    <paramLevelMatches>
      <parameter>satMoisture</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>K</displayUnits>
      <range scale="LINEAR">
        <minValue>-15</minValue>
        <maxValue>15</maxValue>
      </range>
      <defaultColormap>GOES-R/IR/dust_and_moisture_split_window</defaultColormap>
      <colorbarLabeling>
        <values>-12 -8 -4 0 4 8 12 </values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>

<!--Accompanies Sat Fog Channel Difference -->
<styleRule>
    <paramLevelMatches>
      <parameter>satFog</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>K</displayUnits> 
      <range scale="LINEAR">
        <minValue>-15</minValue>
        <maxValue>15</maxValue>
      </range>
      <defaultColormap>GOES-R/IR/fogdiff_blue</defaultColormap>
      <colorbarLabeling>
        <values>-12 -8 -4 0 4 8 12 </values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>

<styleRule>
    <paramLevelMatches>
      <parameter>satDayFog</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>K</displayUnits> 
      <range scale="LINEAR">
        <minValue>-5</minValue>
        <maxValue>60</maxValue>
      </range>
      <defaultColormap>Grid/gridded data</defaultColormap>
      <colorbarLabeling>
        <values>-5 0 5 10 20 30 40 50 </values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>

<styleRule>
    <paramLevelMatches>
      <parameter>SplitWV</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>K</displayUnits>
      <range scale="LINEAR">
        <minValue>-35</minValue>
        <maxValue>5</maxValue>
      </range>
      <defaultColormap>Grid/gridded data</defaultColormap>
      <colorbarLabeling>
        <values>-35 -25 -15 -5 0 5 </values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>

<styleRule>
    <paramLevelMatches>
      <parameter>satSplitOzone</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>K</displayUnits>
      <range scale="LINEAR">
        <minValue>-45</minValue>
        <maxValue>5</maxValue>
      </range>
      <defaultColormap>Grid/gridded data</defaultColormap>
      <colorbarLabeling>
        <values>-45 -35 -25 -15 -5 0 5 </values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>

<styleRule>
    <paramLevelMatches>
      <parameter>satSplitCloudTopPhase</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>K</displayUnits>
      <range scale="LINEAR">
        <minValue>-10</minValue>
        <maxValue>25</maxValue>
      </range>
      <defaultColormap>Grid/gridded data</defaultColormap>
      <colorbarLabeling>
        <values>-5 0 5 10 15 20</values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>

<styleRule>
    <paramLevelMatches>
      <parameter>satSplitSnow</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>%</displayUnits> 
      <range scale="LINEAR">
        <minValue>-75</minValue>
        <maxValue>25</maxValue>
      </range>
      <defaultColormap>Grid/gridded data</defaultColormap>
      <colorbarLabeling>
        <values>-70 -60 -50 -40 -30 -20 -10 0 10 20 </values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>

<styleRule>
    <paramLevelMatches>
      <parameter>satSplitFire</parameter>
    </paramLevelMatches>
    <imageStyle>
      <displayUnits>%</displayUnits> 
      <range scale="LINEAR">
        <minValue>50</minValue>
        <maxValue>-25</maxValue>
      </range>
      <defaultColormap>GOES-R/IR/enhanced-rainbow-11</defaultColormap>
      <colorbarLabeling>
        <values>40 30 20 10 0 -10 -20</values>
      </colorbarLabeling>
    </imageStyle>
  </styleRule>

</styleRuleset>
